{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "off",
        "no-underscore-dangle":"off",
        "linebreak-style":"off",
        "class-methods-use-this":"off",
        "no-console":"off",
        "no-unused-vars":"off",
        "max-len":"off",
        "camelcase":"off"
    }
}
